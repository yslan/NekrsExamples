[OCCA]
backend = CUDA
deviceNumber = 0 #LOCAL-RANK

[GENERAL] 
polynomialOrder = 7
startFrom = ref.fld # frozen flow
stopAt = endTime
endTime = 100.0
dt = 1e-03
timeStepper = bdf2
writeControl = runTime
writeInterval = 1.0

[PRESSURE]
solver=none

[VELOCITY]
solver=none

[TEMPERATURE]
residualTol = 1e-10
viscosity = -1000
